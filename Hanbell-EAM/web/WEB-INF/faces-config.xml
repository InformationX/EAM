            <from-outcome>assetrecycle</from-outcome>
            <to-view-id>/app/ai/assetrecycle.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case>
            <description>AI530资产入库作业</description>
            <from-outcome>assetaccept</from-outcome>
            <to-view-id>/app/ai/assetaccept.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case>
            <description>AI710资产盘点作业</description>
            <from-outcome>assetcheck</from-outcome>
            <to-view-id>/app/ai/assetcheck.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case>
            <description>AI810资产报废作业</description>
            <from-outcome>assetscrap</from-outcome>
            <to-view-id>/app/ai/assetscrap.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case>
            <description>AI820资产处置作业</description>
            <from-outcome>assetdispose</from-outcome>
            <to-view-id>/app/ai/assetdispose.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <description>资产申请一级导航</description>
        <navigation-case>
            <from-outcome>assetapply</from-outcome>
            <to-view-id>/app/aa/assetapply.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case>
            <from-outcome>assetthrow</from-outcome>
            <to-view-id>/app/aa/assetthrow.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <description>模态开窗</description>
        <navigation-case>
            <from-outcome>assetcardSelect</from-outcome>
            <to-view-id>/app/df/assetcardSelect.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case>
            <from-outcome>assetcategorySelect</from-outcome>
            <to-view-id>/app/df/assetcategorySelect.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case>
