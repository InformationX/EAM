            <redirect/>
        </navigation-case>
        <navigation-case>
            <from-outcome>departmentSelect</from-outcome>
            <to-view-id>/app/df/departmentSelect.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case>
            <from-outcome>sysuserSelect</from-outcome>
            <to-view-id>/app/df/sysuserSelect.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case>
            <from-outcome>unitSelect</from-outcome>
            <to-view-id>/app/df/unitSelect.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case>
            <from-outcome>warehouseSelect</from-outcome>
            <to-view-id>/app/df/warehouseSelect.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
    </navigation-rule>
</faces-config>