<?xml version="1.0" encoding="utf-8"?>
<faces-config version="2.0" xmlns="http://java.sun.com/xml/ns/javaee"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd">
    <lifecycle>
        <phase-listener>
            cn.hanbell.eam.web.SignCheck
        </phase-listener>
    </lifecycle>
    <name>primefaces_ultima</name>
    <component>
        <component-type>org.primefaces.component.UltimaMenu</component-type>
        <component-class>org.primefaces.ultima.component.menu.UltimaMenu</component-class>
    </component>
    <render-kit>
        <renderer>
            <component-family>org.primefaces.component</component-family>
            <renderer-type>org.primefaces.component.UltimaMenuRenderer</renderer-type>
            <renderer-class>org.primefaces.ultima.component.menu.UltimaMenuRenderer</renderer-class>
        </renderer>
    </render-kit>
    <application>
        <action-listener>
            org.primefaces.application.DialogActionListener
        </action-listener>
        <navigation-handler>
            org.primefaces.application.DialogNavigationHandler
        </navigation-handler>
        <view-handler>
            org.primefaces.application.DialogViewHandler
        </view-handler>
    </application>
    <navigation-rule>
        <description>入口导航</description>
        <from-view-id>/*</from-view-id>
        <navigation-case>
            <from-outcome>home</from-outcome>
            <to-view-id>/home.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case>
            <from-outcome>login</from-outcome>
            <to-view-id>/login.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case>
            <from-outcome>resetPwd</from-outcome>
            <to-view-id>/resetPwd.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case>
            <from-outcome>404</from-outcome>
            <to-view-id>/404.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case>
            <from-outcome>deny</from-outcome>
            <to-view-id>/deny.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case>
            <from-outcome>error</from-outcome>
            <to-view-id>/error.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <description>基础模块一级导航</description>
        <from-view-id>/*</from-view-id>
        <navigation-case>
            <from-outcome>companygrant</from-outcome>
            <to-view-id>/app/bd/companygrant.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case>
            <from-outcome>assetcard</from-outcome>
            <to-view-id>/app/bd/assetcard.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case>
            <from-outcome>assetcategory</from-outcome>
            <to-view-id>/app/bd/assetcategory.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case>
            <from-outcome>assetitem</from-outcome>
            <to-view-id>/app/bd/assetitem.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case>
            <from-outcome>assetposition</from-outcome>
            <to-view-id>/app/bd/assetposition.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case>
            <from-outcome>unit</from-outcome>
            <to-view-id>/app/bd/unit.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case>
            <from-outcome>warehouse</from-outcome>
            <to-view-id>/app/bd/warehouse.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <description>资产库存一级导航</description>
        <navigation-case>
            <description>AI110交易类别维护</description>
            <from-outcome>inventorytype</from-outcome>
            <to-view-id>/app/ai/inventorytype.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case>
            <description>AI110交易类别维护</description>
            <from-outcome>transactiontype</from-outcome>
            <to-view-id>/app/ai/transactiontype.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case>
            <description>AI150资产库存查询</description>
            <from-outcome>assetinventory</from-outcome>
            <to-view-id>/app/ai/assetinventory.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case>
            <description>AI155交易过程查询</description>
            <from-outcome>assettransaction</from-outcome>
            <to-view-id>/app/ai/assettransaction.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case>
            <description>AI310资产领用作业</description>
            <from-outcome>assetdistribute</from-outcome>
            <to-view-id>/app/ai/assetdistribute.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case>
            <description>AI320资产异动作业</description>
            <from-outcome>assetadjust</from-outcome>
            <to-view-id>/app/ai/assetadjust.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case>
